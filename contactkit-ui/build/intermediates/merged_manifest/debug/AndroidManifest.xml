<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.netease.yunxin.kit.contactkit.ui" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="30" />

    <application>
        <provider
            android:name="com.netease.yunxin.kit.corekit.startup.InitializationProvider"
            android:authorities="${applicationId}.xkit-startup"
            android:exported="false"
            tools:node="merge" >
            <meta-data
                android:name="com.netease.yunxin.kit.contactkit.ui.ContactUIService"
                android:value="xkit.startup" />
        </provider>

        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.contact.ContactActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.addfriend.AddFriendActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.blacklist.BlackListActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.team.TeamListActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.verify.VerifyListActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.userinfo.UserInfoActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.userinfo.CommentActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.netease.yunxin.kit.contactkit.ui.selector.ContactSelectorActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />
    </application>

</manifest>